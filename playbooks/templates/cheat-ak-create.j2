#!/bin/bash

echo "Creating Activation Keys {{ myActivationKeyName_1 }} and {{ myActivationKeyName_2 }}"

hammer activation-key create \
  --name "{{ myActivationKeyName_1 }}" \
  --description "" \
  --lifecycle-environment "{{ myLifecycleName_1 }}" \
  --content-view "{{ myContentViewName }}"  \
  --organization "{{ myOrganization }}"

hammer activation-key create \
  --name "{{ myActivationKeyName_2 }}" \
  --description "" \
  --lifecycle-environment "{{ myLifecycleName_3 }}" \
  --content-view "{{ myContentViewName }}"  \
  --organization "{{ myOrganization }}"
